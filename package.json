{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "preview": "npm run build && next start -p 4000",
    "watch:build": "chokidar \"src/**/*\" \"public/**/*\" \"next.config.*\" \"tsconfig.json\" \"**/*.css\" -c \"npm run build\"",
    "watch:preview": "concurrently -k -n BUILD,START -c blue,green \"npm:watch:build\" \"nodemon --watch .next --ext js,json --delay 200ms --exec \\\"next start -p 4000\\\"\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@ebay/nice-modal-react": "^1.2.13",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "googleapis": "^155.0.1",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "nodemon": "^3.1.7",
    "sharp": "^0.34.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
